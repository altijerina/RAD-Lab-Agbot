        <joint name="elbow_joint" value="-2.482"/>
        <joint name="mid_rail_to_arm_car_joint" value="1.143"/>
        <joint name="shoulder_joint" value="3.8011"/>
        <joint name="wrist_joint" value="-1.3277"/>
    </group_state>
    <group_state name="arm_grab_high_left" group="arm">
        <joint name="arm_base_joint" value="-1.7183"/>
        <joint name="elbow_joint" value="-2.482"/>
        <joint name="mid_rail_to_arm_car_joint" value="1.143"/>
        <joint name="shoulder_joint" value="2.2541"/>
        <joint name="wrist_joint" value="0.1822"/>
    </group_state>
    <group_state name="arm_grab_high_right" group="arm">
        <joint name="arm_base_joint" value="1.1629"/>
        <joint name="elbow_joint" value="-2.482"/>
        <joint name="mid_rail_to_arm_car_joint" value="1.143"/>
        <joint name="shoulder_joint" value="2.2541"/>
        <joint name="wrist_joint" value="0.1822"/>
    </group_state>
    <group_state name="gripper_ready" group="gripper">
        <joint name="gripper_right_joint" value="-0.7637"/>
        <joint name="wrist_joint" value="-1.3277"/>
    </group_state>
    <group_state name="gripper_grab" group="gripper">
        <joint name="gripper_right_joint" value="-0.0955"/>
        <joint name="wrist_joint" value="-1.3277"/>
    </group_state>

