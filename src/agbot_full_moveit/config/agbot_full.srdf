        <joint name="elbow_joint" value="-2.482"/>
        <joint name="mid_rail_to_arm_car_joint" value="1.143"/>
        <joint name="shoulder_joint" value="3.8011"/>
        <joint name="wrist_joint" value="-1.3277"/>
