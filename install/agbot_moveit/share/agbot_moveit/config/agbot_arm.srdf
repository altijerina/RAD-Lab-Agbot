<?xml version="1.0" encoding="UTF-8"?>

<robot name="agbot_arm">

    <group name="arm">
        <joint name="virtual_joint"/>

        <joint name="base_joint"/>
        <joint name="shoulder_joint"/>
        <joint name="elbow_joint"/>
        <joint name="wrist_joint"/>     
    </group>    

    <!-- <group name="all">
        <joint name="base_joint"/>
