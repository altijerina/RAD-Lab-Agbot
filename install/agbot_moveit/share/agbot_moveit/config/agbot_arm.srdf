    <!--END EFFECTOR: Purpose: Represent information about an end effector.-->
    <end_effector name="gripper" parent_link="claw_support" group="gripper"/>

    <!--VIRTUAL JOINT: Purpose: this element defines a virtual joint between a robot 
    link and an external frame of reference (considered fixed with respect to the robot)-->
    <virtual_joint name="world" type="fixed" parent_frame="world" child_link="base_link"/>
    
    <!--DISABLE COLLISIONS: By default it is assumed that any link of the robot 
    could potentially come into collision with any other link in the robot. 
    This tag disables collision checking between a specified pair of links. -->
    <disable_collisions link1="base_link" link2="base_plate" reason="Adjacent"/>
    <disable_collisions link1="base_link" link2="forward_drive_arm" reason="Never"/>
    <disable_collisions link1="base_plate" link2="forward_drive_arm" reason="Adjacent"/>
    <disable_collisions link1="base_plate" link2="gripper_left" reason="Never"/>
    <disable_collisions link1="base_plate" link2="rgb_camera" reason="Never"/>
    <disable_collisions link1="claw_support" link2="forward_drive_arm" reason="Never"/>
    <disable_collisions link1="claw_support" link2="gripper_left" reason="Adjacent"/>
    <disable_collisions link1="claw_support" link2="gripper_right" reason="Adjacent"/>
    <disable_collisions link1="claw_support" link2="horizontal_arm" reason="Adjacent"/>
    <disable_collisions link1="claw_support" link2="rgb_camera" reason="Adjacent"/>
    <disable_collisions link1="forward_drive_arm" link2="gripper_left" reason="Never"/>
    <disable_collisions link1="forward_drive_arm" link2="horizontal_arm" reason="Adjacent"/>
    <disable_collisions link1="forward_drive_arm" link2="rgb_camera" reason="Never"/>
    <disable_collisions link1="gripper_left" link2="gripper_right" reason="Never"/>
    <disable_collisions link1="gripper_left" link2="horizontal_arm" reason="Never"/>
    <disable_collisions link1="gripper_left" link2="rgb_camera" reason="Never"/>
    <disable_collisions link1="gripper_right" link2="horizontal_arm" reason="Never"/>
    <disable_collisions link1="gripper_right" link2="rgb_camera" reason="Never"/>
    <disable_collisions link1="horizontal_arm" link2="rgb_camera" reason="Never"/>
</robot>
