    </group> -->

    <group name="gripper">
        <joint name="gripper_left_joint"/>
        <joint name="gripper_right_joint"/>
    </group>
 
    <group_state name="home" group="arm">
        <joint name="base_joint" value="0"/>
        <joint name="elbow_joint" value="0"/>
        <joint name="shoulder_joint" value="0"/>
        <joint name="wrist_joint" value="0"/>
    </group_state>

    <group name="home" group="gripper">
        <joint name="gripper_right_joint" value="0"/>
    </group>
    
    <group_state name="upright" group="arm">
        <joint name="base_joint" value="0"/>
        <joint name="elbow_joint" value="0"/>
        <joint name="shoulder_joint" value="1.9463"/>
        <joint name="wrist_joint" value="0"/>      
    </group_state>

    <group_state name="low_left_pregrab" group="arm">
        <joint name="base_joint" value="1.308"/>
        <joint name="elbow_joint" value="-1.036"/>
        <joint name="shoulder_joint" value="2.724"/>
        <joint name="wrist_joint" value="-1.163"/>      
    </group_state>    

    <group_state name="high_right_pregrab" group="arm">
        <joint name="base_joint" value="-0.968"/>
        <joint name="elbow_joint" value="-2.242"/>
        <joint name="shoulder_joint" value="1.860"/>
        <joint name="wrist_joint" value="0.773"/>      
    </group_state>     

    <group_state name="grab_wide_open" group="gripper">
        <joint name="gripper_right_joint" value="-1.571"/>
    </group_state>

    <group_state name="gripper_ready" group="gripper">
        <joint name="gripper_right_joint" value="-0.3471"/>
    </group_state>

    <group_state name="gripper_grab" group="gripper">
        <joint name="gripper_right_joint" value="-0.0347"/>
    </group_state>

